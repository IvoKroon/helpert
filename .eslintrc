{
  "extends": ["prettier", "airbnb"],
  "plugins": ["react"],
  "rules": {
    "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
    "jsx-a11y/anchor-is-valid": ["error", { "components": ["Link"], "specialLink": ["to"] }]
  },
  "globals": {
    "window": true,
    "document": true,
    "FileReader": true,
    "localStorage": true
  },
  "parser": "babel-eslint"
}
